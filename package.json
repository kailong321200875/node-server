{
  "name": "base-template",
  "version": "1.0.0",
  "description": "A basic template, including husky, commitlint, prettier",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/main.ts",
    "npm:up": "npx npm-check-updates",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "lint": "eslint .",
    "lint:eslint": "eslint . --fix",
    "lint:format": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "release": "bumpp --commit \"release: release v%s\" --push --tag && npm publish",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "server",
    "api",
    "koa"
  ],
  "author": "Archer <502431556@qq.com>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-static": "^4.0.2",
    "@types/koa__router": "^8.0.11",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "async-validator": "^4.2.5",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "koa-parameter": "^3.0.1",
    "koa-static": "^5.0.0",
    "mongoose": "^6.3.8"
  }
}
